# Useful
alias aida64="am start -n com.finalwire.aida64/.HHMainActivity"
alias adguard="am start -n com.adguard.android/.ui.activity.MainActivity"
alias apktool-m="am start -n ru.maximoff.apktool/.SplashActivity"
alias apktool="apktool-m" # Apktool M
alias authenticator="am start -n com.google.android.apps.authenticator2/com.google.android.apps.authenticator.AuthenticatorActivity"
